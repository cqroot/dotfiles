super + Return
	alacritty

super + shift + Return
	alacritty --class alacritty-float

super + p
	sh ~/.bin/rofi/power.sh

super + s
	rofi -show drun -show-icons -no-history

XF86Audio{LowerVolume,RaiseVolume,Mute}
	pulseaudio-ctl {down,up,mute}; sh ~/.bin/dunst-volume-notify.sh

super + shift + r
	pkill -USR1 -x sxhkd
